<div class="container">

  <h3>Input parameter information:</h3>

  <div class="panel-body">
    <div class="media">
      <div class="media-body col-sm-12">
        <p>
          <strong>Name:</strong>
          <%= @results_app_parameter.name %>
        </p>

        <p>
          <strong>Value:</strong>
          <%= @results_app_parameter.value %>
        </p>

        <%= link_to 'Edit', edit_results_app_results_app_parameter_path(@results_app,@results_app_parameter), class: 'btn btn-xs btn-warning' %> |
        <%= link_to 'Input Parameter List', results_app_path(@results_app), class: 'btn btn-xs btn-default'%>
      </div>
    </div>
  </div>
</div>