<div class="container">
  <h1>
    Analytic server information
  </h1>
  <p>
    <strong>Name:</strong>
    <%= @analytic_server.name %>
  </p>

  <p>
    <strong>IP:</strong>
    <%= @analytic_server.ip %>
  </p>

  <p>
    <strong>Port:</strong>
    <%= @analytic_server.port %>
  </p>

  <p>
    <strong>Status:</strong>
    <% if @analytic_server.status.blank? %>
      <td><%= 'Unknown' %></td>
    <% else %>
      <td><%= @analytic_server.status %></td>
    <% end %>
  </p>

  <p>
    <strong>Process ID:</strong>
    <% if @analytic_server.pid.blank? %>
      <td><%= 'NA' %></td>
  <% else %>
      <td><%= @analytic_server.pid %></td>
  <% end %>
  </p>

  <%# link_to 'Edit', edit_analytic_server_path(@analytic_server) %>
  <%#link_to 'Back', analytic_servers_path %>
</div>